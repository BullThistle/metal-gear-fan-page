<% content_for(:background, "characters") %>
<% content_for(:homelink, "nounderline") %>
<% content_for(:platformlink, "nounderline") %>
<% content_for(:gameslink, "nounderline") %>
<% content_for(:mechslink, "nounderline") %>
<% content_for(:characterslink, "underline") %>
<% content_for(:aboutlink, "nounderline") %>
<div class="center-block">
  <div class="breadcrumbs">
    <nav aria-label="You are here:" role="navigation">
      <ul class="breadcrumbs">
        <li><a href="/">Home</a></li>
        <li><a href="/characters">Characters</a></li>
      </ul>
    </nav>
  </div><div class="middle-content-section">
    <div class="middle-content-text">
      <h1>Characters</h1>
      <ul>
        <% @characters.each do |character| %>
          <li><%= link_to character.char_name, character_path(character) %></li>
        <% end %>
      </ul>
      <hr>
      <%= link_to "New Character", new_character_path %>
    </div>
  </div>
</div>
